{
  "python.jediEnabled": false,
  // Prettier options
  "coc.preferences.formatOnSaveFiletypes": ["typescript"],
  // TabNine options
  "tabnine.disable_filetypes": ["vimwiki", "javascript", "vim", "markdown", "yaml"],
  // Javascript options
  "tsserver.enable": true,
  "tsserver.npm": "/usr/local/bin/npm",
  // Eslint options
  "eslint.enable": false,
  "eslint.run": "onSave",
  // YAML options
  "yaml.schemas": {
    "kubernetes": "/*.yaml",
    "https://raw.githubusercontent.com/docker/cli/master/cli/compose/schema/data/config_schema_v3.9.json": ["/*compose.yml"]
  },
  // LSP
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
  "java.format.settings.url": "/home/dangelo/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml"
}
