{
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j","j"],
            "after": ["<Esc>"],
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["<leader>","S"],
            "after": [],
            "commands":[
                {
                    "command": "editor.emmet.action.wrapWithAbbreviation"
                }
            ]
        },
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["<leader>","d"],
            "after": ["\"","_","d"]
        },
        {
            "before": ["<leader>","x"],
            "after": ["\"","_","x"]
        },
        {
            "before": ["<leader>","c"],
            "after": ["\"","_","c"]
        },
        {
            "before": ["<leader>","f"],
            "after": ["<leader>","<leader>","f"]
        },
        {
            "before": ["<leader>","F"],
            "after": ["<leader>","<leader>","F"]
        },
        {
            "before":["<leader>","q"],
            "after":[],
            "commands": [
                {
                    "command": "workbench.action.closeActiveEditor"
                }
            ]
        },
        {
            "before":["<leader>","u"],
            "after":[],
            "commands": [
                {
                    "command": ":write"
                }
            ]
        },
        {
            "before":["<C-c>"],
            "after":[],
            "commands": [
                {
                    "command": ":nohl"
                }
            ]
        },
        {
            "before":["<C-w>","j"],
            "after":[],
            "commands": [
                {
                    "command": "workbench.action.terminal.resizePaneDown"
                }
            ]
        },
        {
            "before":["<C-w>","k"],
            "after":[],
            "commands": [
                {
                    "command": "workbench.action.terminal.resizePaneUp"
                }
            ]
        },
        {
            "before":["<C-w>","="],
            "after":[],
            "commands": [
                {
                    "command": "workbench.action.evenEditorWidths"
                }
            ]
        },
        {
            "before":["<C-w>","+"],
            "after":[],
            "commands": [
                {
                    "command": "workbench.action.minimizeOtherEditors"
                }
            ]
        },
        {
            "before":["<leader>","e","s"],
            "after":[],
            "commands":[
                {
                    "command": "workbench.action.openSettingsJson"
                }
            ]
        },
        {
            "before":["<leader>","e","k"],
            "after":[],
            "commands":[
                {
                    "command": "workbench.action.openGlobalKeybindingsFile"
                }
            ]
        },
        {
            "before":["<leader>","="],
            "after":[],
            "commands":[
                {
                    "command": "editor.action.formatDocument"
                }
            ]
        },
    ],
    "vim.leader": "<space>",
    "vim.handleKeys":{
        "<C-k>": false,
        "<C-d>": false,
        // "<C-w>": false,
        "<C-f>": false
    },
    "workbench.colorCustomizations": {
        "statusBar.background": "#268bd2",
        "statusBar.noFolderBackground": "#268bd2",
        "statusBar.debuggingBackground": "#268bd2"
    },
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors" : {
        "normal": "#268bd2",
        "insert": "#d33682",
        "visual": "#268bd2",
        "visualline": "#2aa198",
        "visualblock": "#859900",
        "replace": "#b58900"
},
"window.zoomLevel": -2,
"editor.lineNumbers": "relative",
"editor.renderIndentGuides": true,
"editor.multiCursorModifier": "ctrlCmd",
"terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
"git.autofetch": true,
"git.confirmSync": false,
"git.enableSmartCommit": true,
"cosmosDB.showSavePrompt": false,
"editor.minimap.enabled": false
// "guides.indent.backgrounds": ["rgba(10, 10, 10, .4)","rgba(20, 20, 20, .4)"],
// "guides.indent.showFirstIndentGuides": true,
// "guides.stack.enabled": true
}